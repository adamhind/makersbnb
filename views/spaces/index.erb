<html>
<style type="text/css">body { cursor: url('data:image/x-icon;base64,'), auto; }</style>

<ul>
  <% @spaces.each do |space| %>
    <li>
      <%= space.name %>
      <%= space.description %>
      <%= space.price %>

      <form action = '/spaces/request' method = 'POST'>
        <input type = 'hidden' name = 'name' value = '<%=space.name%>'>
        <input type = 'hidden' name = 'space_id' value = '<%=space.id%>'>
        <input type = 'submit' value = 'Request <%=space.name%>'>
      </form>
    </li>
  <% end %>
 </ul>

 <form action = '/spaces/new' method = 'GET'>
  <input type='submit' value='Add a space'>
 </form>
</html>
